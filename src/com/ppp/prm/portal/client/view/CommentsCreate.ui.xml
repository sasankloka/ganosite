<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="constants"
		type="com.ppp.prm.portal.client.messages.MessagesConst" />
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<!-- <g:Label text="{constants.insertComment}"/> -->
			<g:TextArea ui:field="CommentsTxt" />
			<g:Button styleName="{style.important}" ui:field="saveBoutton"
				text="{constants.Save}" />
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 