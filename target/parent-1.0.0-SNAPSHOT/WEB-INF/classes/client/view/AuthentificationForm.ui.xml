<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="constants"
		type="com.ppp.prm.portal.client.messages.MessagesConst" />


	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more
		details */
		.important {
		font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:Label text="{constants.Login}" />
			<g:TextBox ui:field="loginTxt" />
			<g:Label text="{constants.Password}" />
			<g:TextBox ui:field="passTxt" />

			<g:Button styleName="{style.important}" ui:field="LoginBoutton"
				text="{constants.Login}" />
		</g:VerticalPanel>

		<!-- g:HTML wordWrap="true" ui:field="blocHtml"/ -->
	</g:HTMLPanel>
</ui:UiBinder> 